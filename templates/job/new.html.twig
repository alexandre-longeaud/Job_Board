{% extends 'base.html.twig' %}

{% block title %}Hello JobController!{% endblock %}

{% block body %}

    <div class="bg-gradient-to-r from-indigo-500 via-purple-500 to-pink-500 p-4">
        <h1 class="text-4xl text-center">Formulaire</h1>
    </div>

    <div class="container mx-auto px-4 m-4">

        {{ form_start(form) }}

        {{ form_row(form.nom) }}

        {{ form_row(form.description) }}

        {{ form_row(form.salaire) }}

        {{ form_row(form.service) }}

        {{ form_row(form.tags) }}

        
        {{ form_end(form) }}
        
    </div>
 
 

{% endblock %}
